<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Interviewprep"
        tools:targetApi="31">
        <activity
            android:name=".dualpane.ItemDetailHostActivity"
            android:exported="false"
            android:label="@string/title_activity_item_detail_host"
            android:resizeableActivity="true"
            tools:targetApi="24" />
        <activity
            android:name=".RecyclerViewExample"
            android:exported="true">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity
            android:name=".AnimationActivity"
            android:exported="false"
            android:label="@string/title_activity_state_saver"
            android:theme="@style/Theme.Interviewprep" />
        <activity
            android:name=".CoRoTestActivity"
            android:exported="true"
            android:label="@string/title_activity_animation"
            android:theme="@style/Theme.Interviewprep">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/title_activity_co_ro_test"
            android:theme="@style/Theme.Interviewprep" />
        <activity
            android:name=".CoRoActivity"
            android:exported="true"
            android:label="@string/title_activity_co_ro"
            android:theme="@style/Theme.Interviewprep" />
        <activity
            android:name=".StateSaverActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:taskAffinity=".StateSaverActivity"
            android:theme="@style/Theme.Interviewprep" />
    </application>

</manifest>